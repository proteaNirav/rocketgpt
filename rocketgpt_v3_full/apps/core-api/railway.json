{
  "build": {
    "builder": "NIXPACKS",
    "nixpacksPlan": {
      "phases": {
        "install": {
          "cmds": [
            "pip install -r requirements.txt"
          ]
        },
        "start": {
          "cmds": [
            "uvicorn server_stub_fastapi:app --host 0.0.0.0 --port $PORT"
          ]
        }
      }
    }
  }
}