# Enforce consistent line endings in repo
* text=auto eol=lf

# Code
*.ts   text eol=lf
*.tsx  text eol=lf
*.js   text eol=lf
*.jsx  text eol=lf
*.json text eol=lf
*.md   text eol=lf
*.yml  text eol=lf
*.yaml text eol=lf

# Windows scripts
*.ps1  text eol=crlf
*.cmd  text eol=crlf
*.bat  text eol=crlf
apps/core-api/replay/replay_runner.py text eol=lf
